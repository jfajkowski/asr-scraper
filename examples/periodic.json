{
  "name": "wp-scraper",
  "url": "https://www.wp.pl/",
  "interval": 10,
  "output_file": "res/titles.txt",
  "contents_x_paths": [
    "//*[@id=\"text_topnews\"]/li/a/div/text()"
  ],
  "subscrapers": [
    {
      "subscraper_urls_x_paths": [
        "//*[@id=\"text_topnews\"]/li/a/@href"
      ],
      "subscraper_config": {
        "name": "wp-subscraper",
        "output_file": "res/articles.txt",
        "contents_x_paths": [
          ".//*[contains(@class, 'article--title') or contains(@class, 'article--lead') or contains(@class, 'article--text')]"
        ]
      }
    }
  ]
}